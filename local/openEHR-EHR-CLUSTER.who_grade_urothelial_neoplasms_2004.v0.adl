definition
	CLUSTER[at0000] matches {    -- WHO histological grade of urothelial neoplasms (2004/2016)
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Grade
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0006,    -- Urothelial carcinoma in situ
							at0007,    -- Papillary urothelial carcinoma, low grade
							at0008,    -- Papillary urothelial carcinoma, high grade
							at0009,    -- Papillary urothelial neoplasm of low malignant potential
							at0010,    -- Urothelial papilloma
							at0011,    -- Inverted urothelial papilloma
							at0012,    -- Urothelial proliferation of uncertain malignant potential (hyperplasia)
							at0013]    -- Urothelial dysplasia
						}
					}
				}
			}
		}
	}

