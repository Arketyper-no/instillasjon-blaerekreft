definition
	CLUSTER[at0000] matches {    -- Catheter/tube properties
		items cardinality matches {0..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Material
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- Silicone
							at0003,    -- Latex
							at0004,    -- Polyurethane
							at0005,    -- Rubber
							at0006,    -- PVC
							at0007]    -- Other
						}
					}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Overall length
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"cm">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0009] occurrences matches {0..1} matches {    -- External diameter
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|>=0.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {    -- Radiopaque
				value matches {
					DV_BOOLEAN matches {*}
				}
			}
			CLUSTER[at0011] occurrences matches {0..*} matches {    -- Per lumen
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Lumen identifier
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Internal diameter
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Lumen length
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"cm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Lumen purpose
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0016,    -- Airway
									at0017,    -- Medication delivery
									at0018,    -- Fluid administration
									at0019,    -- Monitoring/pressure
									at0020,    -- Drainage
									at0021]    -- Other
								}
							}
						}
					}
					CLUSTER[at0022] occurrences matches {0..1} matches {    -- Connector
						items cardinality matches {0..*; unordered} matches {
							ELEMENT[at0023] occurrences matches {0..1} matches {    -- Connector type
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0024,    -- Push-fit
											at0025,    -- Luer-lock
											at0026,    -- Slip-fit
											at0027]    -- Other
										}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Connector size
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::122]>
										list = <
											["1"] = <
												units = <"mm">
												magnitude = <|>=0.0|>
											>
										>
									>
									DV_CODED_TEXT matches {*}
								}
							}
						}
					}
				}
			}
			CLUSTER[at0029] occurrences matches {0..*} matches {    -- Cuff/balloon
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Cuff type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0031,    -- High pressure/low volume (HPLV)
									at0032]    -- Low pressure/high volume (LPHV)
								}
							}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Cuff position
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0034,    -- Proximal
									at0035]    -- Distal
								}
							}
						}
					}
					ELEMENT[at0036] occurrences matches {0..1} matches {    -- Cuff capacity
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::129]>
								list = <
									["1"] = <
										units = <"ml">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0037] occurrences matches {0..1} matches {    -- Inflation pressure range
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

